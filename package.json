{
  "name": "new",
  "version": "0.0.1",
  "license": "MIT",
  "description": "",
  "author": "HiDeoo <github@hideoo.dev> (https://hideoo.dev)",
  "main": "dist/extension.js",
  "scripts": {
    "build": "pnpm clean && node esbuild.config.mjs",
    "lint": "prettier -c . && eslint . --cache --max-warnings=0 && tsc --noEmit",
    "test": "pnpm clean && tsc && node dist/tests/index.js",
    "clean": "rimraf dist",
    "prepare": "husky install"
  },
  "dependencies": {
    "braces": "3.0.2",
    "fast-glob": "3.2.12",
    "tiny-invariant": "1.3.1"
  },
  "devDependencies": {
    "@hideoo/eslint-config": "0.9.0",
    "@hideoo/prettier-config": "0.1.1",
    "@hideoo/tsconfig": "0.1.0",
    "@types/braces": "3.0.1",
    "@types/chai": "4.3.3",
    "@types/common-tags": "1.8.1",
    "@types/mocha": "10.0.0",
    "@types/node": "16.11.65",
    "@types/sinon": "10.0.13",
    "@types/vscode": "1.72.0",
    "@vscode/test-electron": "2.1.5",
    "chai": "4.3.6",
    "common-tags": "1.8.2",
    "esbuild": "0.15.10",
    "eslint": "8.25.0",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "mocha": "10.0.0",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "sinon": "14.0.1",
    "typescript": "4.8.4"
  },
  "engines": {
    "vscode": "^1.70.0"
  },
  "packageManager": "pnpm@7.13.4",
  "private": true,
  "publisher": "hideoo",
  "sideEffects": false,
  "keywords": [],
  "homepage": "https://github.com/HiDeoo/new",
  "repository": {
    "type": "git",
    "url": "https://github.com/HiDeoo/new.git"
  },
  "bugs": "https://github.com/HiDeoo/new/issues",
  "activationEvents": [
    "onCommand:new.pick"
  ],
  "contributes": {
    "commands": [
      {
        "command": "new.pick",
        "title": "// TODO"
      }
    ],
    "keybindings": [
      {
        "key": "tab",
        "command": "new.autoCompletionNext",
        "when": "new.autoCompletionAvailable"
      }
    ]
  },
  "displayName": "New",
  "prettier": "@hideoo/prettier-config",
  "lint-staged": {
    "**/*": "prettier -w -u --cache",
    "**/*.{js,jsx,ts,tsx,cjs,mjs}": "eslint --cache --max-warnings=0"
  }
}
